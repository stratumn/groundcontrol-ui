{
  "name": "groundcontrol-ui",
  "private": true,
  "dependencies": {
    "ansi-to-react": "^5.0.0",
    "apollo-link": "^1.2.6",
    "apollo-link-ws": "^1.0.12",
    "debounce": "^1.2.0",
    "farce": "^0.2.6",
    "found": "^0.3.17",
    "found-relay": "^0.3.1",
    "moment": "^2.23.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.0.6",
    "react-moment": "^0.8.4",
    "react-relay": "^1.7.0",
    "react-scripts": "2.1.2",
    "relay-runtime": "^1.7.0",
    "semantic-ui-react": "^0.84.0",
    "subscriptions-transport-ws": "^0.9.15"
  },
  "scripts": {
    "start": "react-scripts start",
    "gen": "relay-compiler --src ./src --schema ../groundcontrol/schema.graphql --language typescript",
    "gen-ci": "relay-compiler --src ./src --schema schema.graphql --language typescript",
    "dev": "concurrently \"yarn gen --watch\" \"yarn start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/debounce": "^1.2.0",
    "@types/enzyme": "^3.1.18",
    "@types/enzyme-adapter-react-16": "^1.0.4",
    "@types/graphql": "^14.0.4",
    "@types/jest": "23.3.11",
    "@types/node": "10.12.18",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/react-relay": "^1.3.9",
    "@types/react-test-renderer": "^16.8.1",
    "@types/relay-runtime": "^1.3.7",
    "add": "^2.0.6",
    "babel-plugin-relay": "^1.7.0",
    "concurrently": "^4.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "graphql": "^14.1.1",
    "jest-enzyme": "^7.0.1",
    "prettier": "1.16.4",
    "relay-compiler": "^1.7.0",
    "relay-compiler-language-typescript": "^1.1.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "yarn": "^1.13.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/__generated__/*",
      "!src/testing/*",
      "!src/serviceWorker.ts",
      "!src/setupTests.ts"
    ]
  }
}
